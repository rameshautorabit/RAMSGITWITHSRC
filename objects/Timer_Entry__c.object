    <fields>
        <fullName>Calculated_Duration__c</fullName>
        <description>Returun the calculated duration in Seconds</description>
        <externalId>false</externalId>
        <formula>IF(ISNULL(Duration__c),   (( EndTime__c - StartTime__c )* 24 * 60 * 60)  , Duration__c)</formula>
        <inlineHelpText>This is the total number of seconds calculated for this activity.</inlineHelpText>
        <label>Calculated Duration</label>
        <precision>18</precision>
        <required>false</required>
        <scale>0</scale>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Number</type>
        <unique>false</unique>
    </fields>
