        <type>Lookup</type>
    </fields>
    <fields>
        <fullName>Credit_Count__c</fullName>
        <description>Set to 1 if the source should be counted</description>
        <externalId>false</externalId>
        <formula>IF(
   OR(Source__c = &quot;Transfer Forward&quot;,
      Source__c = &quot;Tier 2&quot;,
      Source__c = &quot;Callout&quot;,
      Source__c = &quot;Future Process&quot;,
      Source__c = &quot;Case Closed&quot;
    ),
1, 0)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <inlineHelpText>If = 1, then this timer entry counts towards case worked/ case not worked</inlineHelpText>
        <label>Credit Count</label>
        <precision>18</precision>
        <required>false</required>
        <scale>0</scale>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Number</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>Duration_Time__c</fullName>
        <externalId>false</externalId>
        <formula>IF(MOD(Calculated_Duration__c, 3600) / 60 &gt;= 10,
  TEXT(FLOOR(Calculated_Duration__c / 3600)) + &quot;:&quot;,
  TEXT(FLOOR(Calculated_Duration__c / 3600)) + &quot;:0&quot;)
&amp;
IF(MOD(MOD(Calculated_Duration__c,3600),60) &gt;= 10,
  TEXT(FLOOR(MOD(Calculated_Duration__c,3600)/60)) + &quot;:&quot;,
  TEXT(FLOOR(MOD(Calculated_Duration__c,3600)/60)) + &quot;:0&quot;)
&amp;
TEXT(FLOOR(MOD(MOD(Calculated_Duration__c,3600),60)))</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <inlineHelpText>Hours: Minutes: Seconds</inlineHelpText>
        <label>Duration</label>
        <required>false</required>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
