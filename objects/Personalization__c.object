        <externalId>false</externalId>
        <formula>IF( 
OR( 
BEGINS ( OrderLineNumber__r.CW_SKU_ID__r.CW_External_Id__c , &quot;GCA&quot;), 
BEGINS ( OrderLineNumber__r.CW_SKU_ID__r.CW_External_Id__c , &quot;GCBC&quot;), 
BEGINS ( OrderLineNumber__r.CW_SKU_ID__r.CW_External_Id__c , &quot;GCRH&quot;), 
BEGINS ( OrderLineNumber__r.CW_SKU_ID__r.CW_External_Id__c , &quot;GCS&quot;), 
BEGINS ( OrderLineNumber__r.CW_SKU_ID__r.CW_External_Id__c , &quot;GCTN&quot;), 
BEGINS ( OrderLineNumber__r.CW_SKU_ID__r.CW_External_Id__c , &quot;REF&quot;)), 

VALUE( Personalization_Text__c ) , 

null)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
