    <fields>
        <fullName>Display_Sort_Order__c</fullName>
        <description>This field is designed to sort &quot;Same Person&apos; relationships to the top of the sort order in related lists and then sort by tier</description>
        <externalId>false</externalId>
        <formula>IF(ISPICKVAL(Relationship_Type__c, &quot;Same Person&quot;), 9, 0) +
IF(ISPICKVAL(Relationship_Type__c, &quot;Shared Order&quot;), 8, 0) +
IF(Contact__r.Tier_Name__c = &quot;Top Tier&quot;, 4, 0) +
IF(Contact__r.Tier_Name__c = &quot;Tier 2&quot;, 3, 0) + 
IF(Contact__r.Tier_Name__c = &quot;Tier 3&quot;, 2, 0)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Display Sort Order</label>
        <precision>18</precision>
        <required>false</required>
        <scale>0</scale>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Number</type>
        <unique>false</unique>
    </fields>
