        <fullName>Company_Name__c</fullName>
        <description>Shows the Company of the Contact if it&apos;s not a Person Account</description>
        <externalId>false</externalId>
        <formula>IF( ! Contact__r.IsPersonAccount ,Contact__r.Account.Name, &quot;&quot;)</formula>
