    <validationRules>
        <fullName>Active_Contact_Information_is_Primary</fullName>
        <active>true</active>
        <description>Prevents users from making non active contact information primary</description>
        <errorConditionFormula>AND(
OR($Profile.Name = &quot;CSC Advocate&quot; , $Profile.Name = &quot;Gallery&quot;),

AND( Is_Inactive__c = TRUE, Is_Primary__c = TRUE)
)</errorConditionFormula>
        <errorMessage>You cannot mark a primary phone or email address as inactive. Choose or create a different primary phone or email address before marking this one as inactive.</errorMessage>
    </validationRules>
