    <fields>
        <fullName>Agent_Response_Time_Hours__c</fullName>
        <description>Calculates the number of hours from when an agent accepts a case until the first email is sent. Intended to be used in conjunction with Agent Response Time Minutes.</description>
        <externalId>false</externalId>
        <formula>VALUE(
IF( Status_for_Formulas__c = &quot;Sent&quot;, 

IF( 
FLOOR((  MessageDate  -  Parent.In_Progress_Start_Time__c  )*24) &gt; 9, 
TEXT(FLOOR(( MessageDate  -  Parent.In_Progress_Start_Time__c )* 24)), 
&quot;0&quot; &amp; TEXT(FLOOR(( MessageDate  -  Parent.In_Progress_Start_Time__c )*24)) 
),

null

))</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Agent Response Time Hours</label>
        <precision>18</precision>
        <required>false</required>
        <scale>0</scale>
        <type>Number</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>Agent_Response_Time_Minutes__c</fullName>
        <description>Calculates the number of minutes from when an agent accepts a case until the first email is sent. Intended to be used in conjunction with Agent Response Time Hours.</description>
        <externalId>false</externalId>
        <formula>VALUE(
IF( Status_for_Formulas__c = &quot;Sent&quot;, 

IF( 
ROUND(MOD((MessageDate  -  Parent.In_Progress_Start_Time__c  )*1440,60),0) &gt; 9, 
TEXT(ROUND(MOD(( MessageDate  -  Parent.In_Progress_Start_Time__c )* 1440,60),0)), 
&quot;0&quot; &amp; TEXT(ROUND(MOD(( MessageDate  -  Parent.In_Progress_Start_Time__c )*1440,60),0)) 
),

null

))</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Agent Response Time Minutes</label>
        <precision>18</precision>
        <required>false</required>
        <scale>0</scale>
        <type>Number</type>
        <unique>false</unique>
    </fields>
