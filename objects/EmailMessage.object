        <fullName>Status_for_Formulas__c</fullName>
        <description>A field that stores email status that can be used in formulas because Email Status cannot be used easily in formulas.</description>
        <externalId>false</externalId>
        <formula>CASE(Status, 
&quot;New&quot;, &quot;New&quot;, 
&quot;Read&quot;, &quot;Read&quot;, 
&quot;Replied&quot;, &quot;Replied&quot;, 
&quot;Sent&quot;, &quot;Sent&quot;, 
&quot;Forwarded&quot;, &quot;Forwarded&quot;, 
&quot;Draft&quot;, &quot;Draft&quot;, 
null)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
