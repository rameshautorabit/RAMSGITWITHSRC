    <validationRules>
        <fullName>Order_Notification_Internal_Email_Only</fullName>
        <active>true</active>
        <description>The order field Order Notification Visibility must be an internal RH email address.</description>
        <errorConditionFormula>AND(
  NOT(ISBLANK(Order_Notification_Visibility__c)),
  NOT(RIGHT(Order_Notification_Visibility__c, 7) = &quot;@rh.com&quot;),
  NOT(RIGHT(Order_Notification_Visibility__c, 24) = &quot;@restorationhardware.com&quot;)
)</errorConditionFormula>
        <errorDisplayField>Order_Notification_Visibility__c</errorDisplayField>
        <errorMessage>Email address must be in the rh.com or restorationhardware.com domain.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Update_Order_Number_on_Shell_ONLY</fullName>
        <active>true</active>
        <errorConditionFormula>AND(RecordType.Name = &quot;Master&quot;, ISCHANGED(Name))</errorConditionFormula>
        <errorMessage>You cannot change the order number.</errorMessage>
    </validationRules>
