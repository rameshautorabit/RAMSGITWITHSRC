    <fields>
        <fullName>Product_Image_URL_New__c</fullName>
        <externalId>false</externalId>
        <formula>/* Original Version of Formula*/ 
/* IF(ISBLANK(ProductID__c),&quot;No Image Available&quot;,HYPERLINK(&quot;http://media.restorationhardware.com/is/image/rhis/&quot; &amp; ProductID__c,&quot;Click to View Image&quot;)) */ 

/* DN 11DEC14 Updated version leverages updated ecommerce Web Service that returns image based on the SKU */ 
IF( Category__c = &quot;BABY &amp; CHILD&quot;, 
(IF(LEN(TRIM(Name)) &gt; 10, 
HYPERLINK(&quot;http://www.rhbabyandchild.com/resto/imageForSku?skuId=&quot; &amp; LEFT(TRIM(Name),6) &amp; &quot;%20&quot; &amp; 
MID(TRIM(Name), 7, 4) &amp; &quot;%20&quot; &amp; MID(TRIM(Name), 11, 4) ,&quot;Click to View Image&quot;), 
HYPERLINK(&quot;http://www.rhbabyandchild.com/resto/imageForSku?skuId=&quot; &amp; LEFT(TRIM(Name),6) &amp; &quot;%20&quot; &amp; 
MID(TRIM(Name), 7, 4) ,&quot;Click to View Image&quot;))), 

HYPERLINK(&quot;http://www.restorationhardware.com/resto/imageForSku?skuId=&quot; &amp; 
LEFT(TRIM(Name),8) &amp; &quot;%20&quot; &amp; MID(TRIM(Name), 9, 4) ,&quot;Click to View Image&quot;))</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Product Image URL (DEP)</label>
        <required>false</required>
        <trackHistory>false</trackHistory>
        <type>Text</type>
        <unique>false</unique>
