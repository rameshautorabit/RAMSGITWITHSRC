    <validationRules>
        <fullName>Sold_Information_Required</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
  Sold__c, 
  OR(
    AND(
      ISBLANK(Sold_To_First_Name__c),
      ISBLANK(Sold_To_Last_Name__c)
    ),
    ISBLANK(Sold_To_Address_1__c),
    ISBLANK(Sold_To_City__c),
    ISBLANK(Sold_To_State_Province__c),
    ISBLANK(Sold_To_Postal_Code__c),
    ISBLANK(Sold_Date__c),
    ISBLANK(Register__c),
    ISBLANK(Transaction__c),
    ISBLANK(Piece__c),
    ISBLANK(Total_Pieces__c),
    ISBLANK(Gallery_Associate__c)
  )
)</errorConditionFormula>
        <errorMessage>If sold, please provide Client Information, Sold Date, Register #, Transaction #, Gallery Associate, Piece #, and Total Pieces.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Total_Pieces</fullName>
        <active>true</active>
        <description>Make sure Piece # is less than or equal to Total Pieces.</description>
        <errorConditionFormula>Piece__c &gt; Total_Pieces__c</errorConditionFormula>
        <errorDisplayField>Piece__c</errorDisplayField>
        <errorMessage>Piece # cannot be greater than Total Pieces.</errorMessage>
    </validationRules>
