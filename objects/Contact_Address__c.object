    <validationRules>
        <fullName>CannotEditLinkedAddresses</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
 $Profile.Name != &quot;System Administrator&quot;,
 $Profile.Name != &quot;API Only&quot;,

OR(
 Is_Bill_To_Address__c = TRUE,
 Is_Ship_To_Address__c = TRUE,
 is_Sold_To_Address__c = TRUE),

OR(
   ISCHANGED(  Contact__c ),
   ISCHANGED( Address_1__c ),
   ISCHANGED( Address_2__c ),
   ISCHANGED( Address_3__c ),
   ISCHANGED( Address_4__c ),
   ISCHANGED( APARTMENT__c ), 
   ISCHANGED( CITY__c  ), 
   ISCHANGED( State__c ), 
   ISCHANGED( Zip__c ), 
   ISCHANGED( Country__c ), 
   ISCHANGED( Company_Name__c ), 
   ISCHANGED( Contact__c ), 
   ISCHANGED( First_Name__c ), 
   ISCHANGED( Last_Name__c ), 
   ISCHANGED( Email__c ), 
   ISCHANGED( Day_Phone__c ), 
   ISCHANGED( Night_Phone__c ) 
   )
)</errorConditionFormula>
        <errorMessage>You cannot edit an address that is linked to an order as a Sold To, Bill To, or Ship To. You can only mark such addresses as inactive.</errorMessage>
    </validationRules>
