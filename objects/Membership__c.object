        <externalId>false</externalId>
        <formula>if(
OR(ISNULL(Enrollment_Date__c),
   Enrollment_Date__c &gt; TODAY(),
   Expiration_Date__c &lt; TODAY(), 
   ISPICKVAL(Cancellation_Type__c, &quot;RH Terminated&quot;),
   ISPICKVAL(Cancellation_Type__c, &quot;Member Terminated&quot;)
), false, true)</formula>
