    <fields>
        <fullName>Special_Enrollee__c</fullName>
        <description>// SMR-164
// Members who enrolled between 3/12 and 3/16
// According to our T&amp;Cs during this 4 day period (3/12/16-3/16/16), we told 
// customers we would email them at 30 days by default.
// Any Member meeting all of the following conditions: Enrollment Date &lt;= 3/16/16</description>
        <externalId>false</externalId>
        <formula>IF(
AND(Enrollment_Date__c &gt;=  DATE(2016,3,12),Enrollment_Date__c &lt;=  DATE(2016,3,16)),
TRUE, FALSE)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Special Enrollee</label>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Checkbox</type>
