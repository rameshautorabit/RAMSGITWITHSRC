    <validationRules>
        <fullName>AddressFieldsNotBlankifScheduledDate</fullName>
        <active>false</active>
        <errorConditionFormula>AND(

 $Profile.Name &lt;&gt; &quot;System Administrator&quot;, 
 $Profile.Name &lt;&gt; &quot;API Only&quot;,

NOT(ISBLANK( Member_Card_Scheduled_Request_Date__c )),


OR( 
ISBLANK ( Member_Card_Street_1__c ), 
ISBLANK ( Member_Card_City__c ), 
ISBLANK ( Member_Card_State__c ), 
ISBLANK ( Member_Card_Postal_Code__c ), 
ISBLANK ( Member_Card_Country__c ) 
) 
)</errorConditionFormula>
        <errorMessage>You cannot schedule a card to be sent without also setting an address to which the card will be sent.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>CancelReasonReqdwithCancelType</fullName>
        <active>false</active>
        <description>Cancellation Reason is required if a Cancellation Type is selected.</description>
        <errorConditionFormula>AND(
$Profile.Name &lt;&gt; &quot;System Administrator&quot;, 
$Profile.Name &lt;&gt; &quot;API Only&quot;, 
NOT(ISBLANK( TEXT(Cancellation_Type__c ))),
ISBLANK( TEXT( Cancellation_Reason__c ))
)</errorConditionFormula>
        <errorMessage>You must choose a Cancellation Reason when you choose a Cancellation Type.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>CannotCreateCompMemberships</fullName>
        <active>false</active>
        <description>Only Jamie Kasselman, Marcello Smith, and Ashima Seghal can create Comp memberships.</description>
        <errorConditionFormula>AND(
$Profile.Name &lt;&gt; &quot;System Administrator&quot;, 
$Profile.Name &lt;&gt; &quot;API Only&quot;, 

NOT(CONTAINS($Setup.MembershipVRExcludedUser__c.UserId__c, $User.Username)),

OR(
ISCHANGED(Membership_Type__c),

AND(
ISNEW(),
OR(
ISPICKVAL (Membership_Type__c, &quot;Complimentary - PR&quot;),
ISPICKVAL (Membership_Type__c, &quot;Complimentary - VIP&quot;)))
)
)</errorConditionFormula>
        <errorMessage>You cannot create a Complimentary membership or edit a Paid membership and make it Complimentary.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>CannotEditMuleUserCreatedMemberships</fullName>
        <active>false</active>
        <description>Users cannot edit Agreed to T&amp;C and Membership Type on Mule created Memberships</description>
        <errorConditionFormula>AND(
$Profile.Name &lt;&gt; &quot;System Administrator&quot;, 
$Profile.Name &lt;&gt; &quot;API Only&quot;, 
OR(
 CreatedBy.Username = &quot;mule@restorationhardware.com&quot;,
CreatedBy.Username = &quot;sfdcadmin1@restorationhardware.com&quot;),
OR(
ISCHANGED(Membership_Type__c ),
 ISCHANGED(Agreed_T_C__c ))
)</errorConditionFormula>
        <errorMessage>You cannot modify the Membership Type or the Agreed to T&amp;C.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>CannotModifySomeCancelTypeReasons</fullName>
        <active>false</active>
        <description>Prevents users from updating the Cancellation Type and Cancellation Reason from system generated values.</description>
        <errorConditionFormula>AND(
$Profile.Name &lt;&gt; &quot;System Administrator&quot;, 
$Profile.Name &lt;&gt; &quot;API Only&quot;,
$UserRole.Name  &lt;&gt; &quot;Administration&quot;,


OR(

AND(
ISCHANGED (Cancellation_Reason__c),
ISPICKVAL (Cancellation_Reason__c , &quot;Renewal failed&quot;)
),

AND(
ISCHANGED (Cancellation_Reason__c),
ISPICKVAL (Cancellation_Reason__c , &quot;Quebec&quot;)
),


AND(
ISCHANGED (Cancellation_Type__c),
ISPICKVAL (Cancellation_Type__c , &quot;Pending&quot;)
)
)
)</errorConditionFormula>
        <errorMessage>You cannot choose the Cancellation Reasons &quot;Renewal failed&quot; or &quot;Quebec,&quot; and you cannot choose or change the Cancellation Type &quot;Pending.&quot; If you have made one of these selections, please update the Cancellation Type and/or Cancellation Reason.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>CannotSaveWOAgreedTC</fullName>
        <active>false</active>
        <description>Users cannot create paid memberships without checking the Agreed to T&amp;C box.</description>
        <errorConditionFormula>AND(
$Profile.Name &lt;&gt; &quot;System Administrator&quot;, 
$Profile.Name &lt;&gt; &quot;API Only&quot;, 
ISNEW(),
ISPICKVAL(Membership_Type__c, &quot;Paid&quot;),
Agreed_T_C__c = FALSE)</errorConditionFormula>
        <errorMessage>You cannot save Paid memberships without checking the box for Agreed to T&amp;Cs.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>CantEditMemberID</fullName>
        <active>false</active>
        <errorConditionFormula>AND(
$Profile.Name &lt;&gt; &quot;System Administrator&quot;, 
$Profile.Name &lt;&gt; &quot;API Only&quot;,  
PRIORVALUE(Name) != &apos;Automated&apos;,
ISCHANGED(Name))</errorConditionFormula>
        <errorMessage>You cannot edit the Member ID.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ContactNotBlankforCompMemberships</fullName>
        <active>false</active>
        <description>The Contact cannot be blank for complimentary memberships.</description>
        <errorConditionFormula>AND(
$Profile.Name &lt;&gt; &quot;System Administrator&quot;, 
$Profile.Name &lt;&gt; &quot;API Only&quot;, 
ISBLANK ( Contact__c ),

OR(
ISPICKVAL( Membership_Type__c , &quot;Complimentary - PR&quot;),
ISPICKVAL( Membership_Type__c , &quot;Complimentary - VIP&quot;)
)
)</errorConditionFormula>
        <errorMessage>Please update the Contact for this Complimentary Membership. It cannot be left blank.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ExpiredMembersNeedCancellationTypeReason</fullName>
        <active>true</active>
        <description>SME-85. Expired memberships are required to have a Cancellation Type and Reason.</description>
        <errorConditionFormula>AND(
$Profile.Name &lt;&gt; &quot;System Administrator&quot;, 
$Profile.Name &lt;&gt; &quot;API Only&quot;,
$UserRole.Name &lt;&gt; &quot;Administration&quot;,
 Expiration_Date__c &lt; TODAY(),
NOT(ISPICKVAL(Cancellation_Type__c, &quot;Pending&quot;)),
NOT(ISPICKVAL(Cancellation_Reason__c, &quot;Credit card decline&quot;)),

OR(
ISCHANGED( Cancellation_Type__c),
ISCHANGED( Cancellation_Reason__c)
)
)</errorConditionFormula>
        <errorMessage>Cancellation Type and Cancellation Reason cannot be changed for expired memberships.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>MemberAddressFieldsCannotbeBlank</fullName>
        <active>false</active>
        <description>All paid memberships must have an address.</description>
        <errorConditionFormula>AND(
$Profile.Name &lt;&gt; &quot;System Administrator&quot;, 
$Profile.Name &lt;&gt; &quot;API Only&quot;, 

ISPICKVAL (Membership_Type__c, &quot;Paid&quot;),


OR(
ISBLANK ( Member_Street_1__c ),
ISBLANK ( Member_City__c ),
ISBLANK ( Member_State__c ),
ISBLANK ( Member_Postal_Code__c ),
ISBLANK ( Member_Country__c )
)
)</errorConditionFormula>
        <errorMessage>All Paid memberships must have an address.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>MemberCardCountry2Chars</fullName>
        <active>false</active>
        <description>Member Card Country can be no more than 2 characters</description>
        <errorConditionFormula>AND(
$Profile.Name &lt;&gt; &quot;System Administrator&quot;, 
$Profile.Name &lt;&gt; &quot;API Only&quot;, 
(LEN( Member_Card_Country__c )  &gt; 2)
)</errorConditionFormula>
        <errorMessage>Member Card Country must be entered as a country code. For United States, enter US. For Canada, enter CA. For all other countries, consult this list: http://www.nationsonline.org/oneworld/country_code_list.htm</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>PhoneOrderCheckedforSomeApplications</fullName>
        <active>false</active>
        <description>Phone Order will be check only for some options in Created by Application.</description>
        <errorConditionFormula>AND(
$Profile.Name &lt;&gt; &quot;System Administrator&quot;, 
$Profile.Name &lt;&gt; &quot;API Only&quot;,

Phone_Order__c = TRUE,

OR( 
ISPICKVAL (Created_by_Application__c , &quot;RH - Web&quot;),
ISPICKVAL (Created_by_Application__c , &quot;RHBC - Web&quot;),
ISPICKVAL (Created_by_Application__c , &quot;RHTN - Web&quot;),
ISPICKVAL (Created_by_Application__c , &quot;RHMO - Web&quot;),
ISPICKVAL (Created_by_Application__c , &quot;MAUI&quot;)
)
)</errorConditionFormula>
        <errorMessage>Phone Order cannot be checked for the application chosen in Created by Application. Please choose Salesforce.com or one of the Concierge options.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ScheduledDateCannotbeTodayorPast</fullName>
        <active>false</active>
        <description>Member card scheduled request date cannot be updated to today or less than today</description>
        <errorConditionFormula>AND( 
$Profile.Name &lt;&gt; &quot;System Administrator&quot;, 
$Profile.Name &lt;&gt; &quot;API Only&quot;,
ISCHANGED( Member_Card_Scheduled_Request_Date__c ) ,
Member_Card_Scheduled_Request_Date__c  &lt;= TODAY()

)</errorConditionFormula>
        <errorMessage>Card shipment requests may not be edited to be in the past and cannot be scheduled for the same day. Update the Member Card Scheduled Request Date to tomorrow or another future date.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ScheduledDateNotBlankforInFlightCards</fullName>
        <active>false</active>
        <description>Member card scheduled request date cannot be blank if member card status = request sent or card sent</description>
        <errorConditionFormula>AND( 
$Profile.Name &lt;&gt; &quot;System Administrator&quot;, 
$Profile.Name &lt;&gt; &quot;API Only&quot;,
 
ISBLANK( Member_Card_Scheduled_Request_Date__c ),

OR(
 ISPICKVAL(Member_Card_Status__c, &quot;Request Sent&quot;),
 ISPICKVAL(Member_Card_Status__c, &quot;Card Sent&quot;))

)</errorConditionFormula>
        <errorMessage>Either the request to send member card or the card itself has already been sent and may not be canceled by removing the Member Card Scheduled Request Date.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>UpdateScheduledDateforAddressChanges</fullName>
        <active>false</active>
        <description>If the Card shipment address is updated, the Member Card Request Scheduled Date cannot be today or in the past, or less than the ship date.</description>
        <errorConditionFormula>AND(
$Profile.Name &lt;&gt; &quot;System Administrator&quot;, 
$Profile.Name &lt;&gt; &quot;API Only&quot;, 

OR(
ISCHANGED( Member_Card_Street_1__c ),
ISCHANGED( Member_Card_Street_2__c ),
ISCHANGED( Member_Card_City__c ),
ISCHANGED( Member_Card_State__c ),
ISCHANGED( Member_Card_Postal_Code__c ),
ISCHANGED( Member_Card_Country__c )),

OR(
 Member_Card_Scheduled_Request_Date__c &lt;  Membership_Card_Ship_Date__c ,
Member_Card_Scheduled_Request_Date__c &lt;= TODAY())

)</errorConditionFormula>
        <errorMessage>A card shipment may be in process. Updating the Member Card Address fields will not redirect an in process request. If you would like to send a new Member Card, please update the Member Card Scheduled Request Date to tomorrow or another future date.</errorMessage>
    </validationRules>
