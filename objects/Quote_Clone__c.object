    <validationRules>
        <fullName>Amounts_To_Be_Applied_Dont_Exceed_Total</fullName>
        <active>true</active>
        <description>Ensures that the two amounts to be applied do not exceed the Contract Total.</description>
        <errorConditionFormula>Amount_To_Be_Applied__c + Second_Amount_To_Be_Applied__c &gt; Contract_Total__c</errorConditionFormula>
        <errorMessage>The two Amounts To Be Applied cannot exceed the contract total. Please change the Amounts to be Applied</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Card_Number_is_in_a_Valid_Format</fullName>
        <active>true</active>
        <description>Ensures that the Card Number is a Master Card, Visa or American Express card, and is in the correct format for the card.</description>
        <errorConditionFormula>AND
(
    NOT(LEFT(Card_Number__c,2) = &quot;34&quot;),
    NOT(LEFT(Card_Number__c,2) = &quot;37&quot;),
    NOT(LEFT(Card_Number__c,1) = &quot;4&quot;),
    NOT(LEFT(Card_Number__c,1) = &quot;5&quot;),
    OR
    (
        AND
        (
            LEFT(Card_Number__c,1) = &quot;3&quot;,
            NOT(LEN(Card_Number__c) = 15)
        ),
        AND
        (
            OR
            (
                LEFT(Card_Number__c,1) = &quot;4&quot;,
                LEFT(Card_Number__c,1) = &quot;5&quot;
            ),
            NOT(LEN(Card_Number__c) = 16)
        )
    )
)</errorConditionFormula>
        <errorDisplayField>Card_Number__c</errorDisplayField>
        <errorMessage>Credit card number must be a valid Master Card, Visa or American Express card number.</errorMessage>
    </validationRules>
