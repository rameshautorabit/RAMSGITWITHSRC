IF(ISPICKVAL(Urgency_Override__c, &quot;Yellow&quot;),IMAGE(&quot;/resource/Warning&quot;, &quot;warning32&quot;),

IF(ISPICKVAL(Urgency_Override__c, &quot;Red&quot;),IMAGE(&quot;/resource/Error&quot;, &quot;error&quot;),

IF(
AND(OR(RecordType.Name = &quot;Side Mark&quot;,RecordType.Name = &quot;POD&quot;,RecordType.Name = &quot;Freight Request&quot;,RecordType.Name = &quot;Vendor Air Freight&quot;,RecordType.Name = &quot;DC Pickup&quot;),
OR(
(Next_Contact_with_Client__c)-DATEVALUE(CreatedDate)&lt; 21,
DATEVALUE(LastModifiedDate)-DATEVALUE(CreatedDate) = 2)),
IMAGE(&quot;/resource/Warning&quot;, &quot;warning32&quot;),

IF(DATEVALUE(LastModifiedDate)- DATEVALUE(CreatedDate) &gt; 3,IMAGE(&quot;/resource/Error&quot;, &quot;error&quot;),

IF(AND(OR(RecordType.Name = &quot;Side Mark&quot;,RecordType.Name = &quot;POD&quot;,RecordType.Name = &quot;Freight Request&quot;,RecordType.Name = &quot;Vendor Air Freight&quot;,RecordType.Name = &quot;DC Pickup&quot;),(Next_Contact_with_Client__c) - DATEVALUE(CreatedDate) &gt; 21),IMAGE(&quot;/resource/Confirm&quot;, &quot;confirm32&quot;),

IMAGE(&quot;/resource/Confirm&quot;, &quot;confirm32&quot;)
))))))</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
