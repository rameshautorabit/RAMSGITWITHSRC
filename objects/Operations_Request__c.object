        <externalId>false</externalId>
        <formula>IF(ISPICKVAL(Urgency_Override__c , &quot;Green&quot;),&quot;Green&quot;,

IF(ISPICKVAL(Urgency_Override__c, &quot;Yellow&quot;),&quot;Yellow&quot;,

IF(ISPICKVAL(Urgency_Override__c, &quot;Red&quot;),&quot;Red&quot;,

IF(
AND(OR(RecordType.Name = &quot;Side Mark&quot;,RecordType.Name = &quot;POD&quot;,RecordType.Name = &quot;Freight Request&quot;,RecordType.Name = &quot;Vendor Air Freight&quot;,RecordType.Name = &quot;DC Pickup&quot;),
OR(
(Next_Contact_with_Client__c)-DATEVALUE(CreatedDate)&lt; 21,
DATEVALUE(LastModifiedDate)-DATEVALUE(CreatedDate) = 2)),
&quot;Yellow&quot;,

IF(DATEVALUE(LastModifiedDate)- DATEVALUE(CreatedDate) &gt; 3,&quot;Red&quot;,

IF(AND(OR(RecordType.Name = &quot;Side Mark&quot;,RecordType.Name = &quot;POD&quot;,RecordType.Name = &quot;Freight Request&quot;,RecordType.Name = &quot;Vendor Air Freight&quot;,RecordType.Name = &quot;DC Pickup&quot;),(Next_Contact_with_Client__c) - DATEVALUE(CreatedDate) &gt; 21),&quot;Green&quot;,

&quot;Green&quot;
))))))</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
