    <validationRules>
        <fullName>AtLeastOneDayActive</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
  Active__c,
  NOT(Sunday__c),
  NOT(Monday__c),
  NOT(Tuesday__c),
  NOT(Wednesday__c),
  NOT(Thursday__c),
  NOT(Friday__c),
  NOT(Saturday__c)
)</errorConditionFormula>
        <errorMessage>If schedule is active, at least one day must be checked.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>IntervalMinutes</fullName>
        <active>true</active>
        <errorConditionFormula>Interval_Minutes__c &lt; 30</errorConditionFormula>
        <errorDisplayField>Interval_Minutes__c</errorDisplayField>
        <errorMessage>Interval Minutes must be  greater than or equal to 30.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>MaxCasesGreaterThanZero</fullName>
        <active>true</active>
        <errorConditionFormula>Max_Cases__c &lt;= 0</errorConditionFormula>
        <errorDisplayField>Max_Cases__c</errorDisplayField>
        <errorMessage>Max Cases must be greater than zero.</errorMessage>
    </validationRules>
