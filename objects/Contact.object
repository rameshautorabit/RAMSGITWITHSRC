    <validationRules>
        <fullName>OnlyContractCanEditContractContacts</fullName>
        <active>true</active>
        <description>SRS-2071</description>
        <errorConditionFormula>AND(

 $Profile.Name &lt;&gt; &quot;Contract&quot;,
 $Profile.Name &lt;&gt; &quot;Contract Business Administrator&quot;,
 $Profile.Name &lt;&gt; &quot;API Only&quot;,
 $Profile.Name &lt;&gt; &quot;System Administrator&quot;,
 Account.RecordType.DeveloperName = &quot;Contract&quot;)</errorConditionFormula>
        <errorMessage>Only Contract users can edit Contacts associated to Contract Accounts.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>UpdateUnauthorizedContacts</fullName>
        <active>true</active>
        <description>SRS-2071. Gallery and CSC users can only update Unauthorized contacts on Unclassified Business Contacts. Trade and Contract can update on their own Contacts.</description>
        <errorConditionFormula>AND(
 $Profile.Name &lt;&gt; &quot;Trade&quot;,
 $Profile.Name &lt;&gt; &quot;System Administrator&quot;,
 $Profile.Name &lt;&gt; &quot;API Only&quot;,
 $UserRole.Name &lt;&gt; &quot;Administration&quot;,
 Account.RecordType.DeveloperName = &quot;Trade&quot;,
 ISCHANGED( Unauthorized_Contact_Checkbox__c ))</errorConditionFormula>
        <errorMessage>Only Trade users can change Unauthorized Contact on contacts associated to Trade accounts.</errorMessage>
    </validationRules>
