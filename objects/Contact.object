    <fields>
        <fullName>Address_Quality_Description__c</fullName>
        <externalId>false</externalId>
        <formula>IF( Address_Quality_Score__c =100,&quot;All Address Details Captured&quot;, &quot;Missing: &quot; 
&amp;IF( LEN(MailingStreet) = 0, &quot;Street Address, &quot;,&quot;&quot;) 
&amp;IF( LEN(MailingState) = 0, &quot;State, &quot;,&quot;&quot;) 
&amp;IF( LEN(MailingPostalCode) = 0, &quot;Zip Code, &quot;,&quot;&quot;) 
&amp;IF( LEN(MailingCountry) = 0, &quot;Country, &quot;,&quot;&quot;))</formula>
        <label>Address Quality Description</label>
        <required>false</required>
        <trackHistory>false</trackHistory>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>Address_Quality_Score__c</fullName>
        <externalId>false</externalId>
        <formula>100 - 
IF( LEN(MailingStreet)= 0,30,0) - 
IF( LEN(MailingState)= 0,30,0) - 
IF( LEN(MailingPostalCode)= 0,30,0) - 
IF( LEN(MailingCountry)= 0,10,0)</formula>
        <label>Address Quality Score</label>
        <precision>18</precision>
        <required>false</required>
        <scale>0</scale>
        <trackHistory>false</trackHistory>
        <type>Number</type>
        <unique>false</unique>
    </fields>
