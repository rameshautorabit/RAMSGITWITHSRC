<apex:page standardController="Opportunity" extensions="Opportunity_DAQuoteCtrlExt">
    <apex:form >
        <apex:pageBlock mode="detail">
            <apex:pageBlockSection columns="1">
                <apex:pageBlockTable value="{!daQuoteWrapperList}" var="daQW">
                    <apex:column headerValue="Quote">
                    	<apex:outputLink value="{!daQW.conciergeBaseURL}/quote/quote_detail.jsp?quoteId={!daQW.daQuote.Quote_Id__c}" target="_blank">{!daQW.daQuote.Quote_Id__c}</apex:outputLink>
					</apex:column>	 	
                    <apex:column headerValue="Subtotal" value="{!daQW.daQuote.Order_Subtotal__c}"/>
                    <apex:column headerValue="Forecast" value="{!daQW.daQuote.Forecast__c}"/>
                    <apex:column headerValue="Converted" value="{!daQW.daQuote.Converted__c}"/>
               </apex:pageBlockTable>   
            </apex:pageBlockSection>
        </apex:pageBlock>
    </apex:form>
</apex:page>