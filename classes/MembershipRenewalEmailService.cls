  /*
   * Helper method to convert Boolean into a single character value
   */
  
  private static String booleanKeyVal(Boolean value) {
    return value ? 'T' : 'F';
  } 
    
    /*
     * Generate a key from the properties of the membership record
     */
     
    public static String generateMatchKey(MembershipRenewalService.MembershipRenewalProperties mrp) {      
      return
      String.valueOf(mrp.daysOut).leftPad(2, '0') + '-' + 
      booleanKeyVal(mrp.noCC) + '-' +
           booleanKeyVal(mrp.expiringCC) + '-' +
           booleanKeyVal(mrp.validCC) + '-' +
           booleanKeyVal(!mrp.isNonCanada) + '-' +               
           booleanKeyVal(mrp.isQuebecois) + '-' +
           booleanKeyVal(mrp.autoRenew);
    }
    
    /*
     * Get the content based on the generated key
     * If there is an exact match, use that, else try and find a partial match
     */
    
    public static String getContent(String generatedKey, Map<String, String> contentMap) {
      if (contentMap.keySet().contains(generatedKey)) {
        return contentMap.get(generatedKey);        
      }
      for (String contentKey : contentMap.keySet()) {
        if (isKeyMatch(generatedKey, contentKey)) {
          return contentMap.get(contentKey);
        }
      }
      return null;
    }
    
    /*
     * Attempt to match the generated key with the match key
     * Allow for wildcards ("A" stands for "Any")
     */
    
    public static Boolean isKeyMatch(String generatedKey, String matchKey) {
      Boolean matched = true;
      if (!matchKey.equals(generatedKey)) {
        for (Integer index = 0; matched && index < matchKey.length() && index < generatedKey.length(); index ++) {
          matched = 
            matchKey.substring(index,index+1) == 'A' || 
            matchKey.substring(index,index+1) == generatedKey.subString(index,index+1);
        }
    }
    return matched;
    }
