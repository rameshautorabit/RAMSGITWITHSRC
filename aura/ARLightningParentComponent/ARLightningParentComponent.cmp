<aura:component implements="force:appHostable,flexipage:availableForAllPageTypes,flexipage:availableForRecordHome,force:hasRecordId,forceCommunity:availableForAllPageTypes" access="global" >
    
    <ltng:require styles="/resource/SLDS/assets/styles/salesforce-lightning-design-system-ltng.css"/>
    
    <div aura:id="buttonsDiv" align="center">
        <ui:button label="Setup" class="tab" press="{!c.buttonClick}"/>
        <ui:button label="Commit" class="tab" press="{!c.buttonClick}"/>
        <ui:button label="History" class="tab" press="{!c.buttonClick}"/>
    </div> 
    
    
    <div aura:id="replacableDiv" decription="Content replacable div">
        <c:SetupComponent />
    </div>
    
</aura:component>