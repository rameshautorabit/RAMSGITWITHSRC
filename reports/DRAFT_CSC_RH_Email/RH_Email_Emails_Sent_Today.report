    <currency>USD</currency>
    <description>RH emails sent today by associate. The &quot;From Address&quot; may be any of the many RH Email addresses available; webcs@rh.com, babyandchild@rh.com, etc.</description>
    <filter>
        <criteriaItems>
            <column>Case.EmailMessages$FromAddress</column>
            <isUnlocked>true</isUnlocked>
            <operator>contains</operator>
            <value>babyandchild,bcproducts,bcservices,bcsite,webcs,rhgrey@,rhmodern,rhteen,rhmembers</value>
        </criteriaItems>
        <criteriaItems>
            <column>Case.EmailMessages$Incoming</column>
            <isUnlocked>true</isUnlocked>
            <operator>equals</operator>
            <value>0</value>
        </criteriaItems>
    </filter>
    <format>Summary</format>
    <groupingsDown>
        <dateGranularity>Day</dateGranularity>
        <field>Case.EmailMessages$CreatedBy</field>
        <sortOrder>Asc</sortOrder>
    </groupingsDown>
    <name>RH Email: Outbound Emails by Associate</name>
    <params>
        <name>co</name>
        <value>1</value>
    </params>
    <reportType>Cases_with_Emails_Custom__c</reportType>
    <scope>organization</scope>
    <showDetails>true</showDetails>
    <timeFrameFilter>
        <dateColumn>Case.EmailMessages$MessageDate</dateColumn>
        <interval>INTERVAL_TODAY</interval>
    </timeFrameFilter>
</Report>
